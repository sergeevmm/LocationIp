@model IEnumerable<LocationIp.CityBlock>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Network)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GeonameId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RegisteredCountryGeonameId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RepresentedCountryGeonameId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsAnonymousProxy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsSatelliteProvider)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PostalCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Latitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Longitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AccuracyRadius)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Network)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GeonameId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RegisteredCountryGeonameId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RepresentedCountryGeonameId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsAnonymousProxy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsSatelliteProvider)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PostalCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Latitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Longitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccuracyRadius)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CityBlocksId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CityBlocksId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CityBlocksId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
